set xrange [*:*]
set yrange [0.0:0.01]

set key reverse inside left bottom

set title

plot 'POST_VEL.dat' index 12 every  1 using 4:1 w linespoints ls  4 title '   8 mesh', \
     'POST_VEL.dat' index 13 every  2 using 4:1 w linespoints ls  6 title '  16 mesh', \
     'POST_VEL.dat' index 14 every  4 using 4:1 w linespoints ls  8 title '  32 mesh', \
     'POST_VEL.dat' index 15 every  8 using 4:1 w linespoints ls 10 title '  64 mesh'

unset logscale x
unset multiplot




#-------------------------------------------------------------------------------------

