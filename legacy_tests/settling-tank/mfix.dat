   NO_K = .TRUE.
#
# GAS SECTION
#
  MW_g  = 29.                         ! molecular weight of gas species 
  MU_g0 = 0.009                       ! Gas viscosity
  RO_g0  = 1.0

#
# PARTICLE SECTION
#
  e    = 0.8                          ! coefficient of restitution
  phi  = 0.0

  MMAX = 1                            ! No of solids
  D_p0          =  0.1
  RO_s0         =  2.0

  EP_star = 0.4                       ! maximum possible packing

#
# INITIAL CONDITIONS SECTION
#
       !
  IC_x_w        =  0.0
  IC_x_e        = 50.0
  IC_y_s        =  0.0
  IC_y_n        = 50.0

