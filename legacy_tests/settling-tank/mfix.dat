#
#  Granular Flow in a Settling Tank
#
 
#
# RUN CONTROL SECTION
#

  RUN_NAME = 'TANK'
  RUN_TYPE = 'new'
  DESCRIPTION = 'Granular Flow in a Settling Tank'
  UNITS = 'CGS'
  ENERGY_EQ = .FALSE.    ! do not solve energy equations
  SPECIES_EQ = .FALSE.  .FALSE.

   TIME = 0.0
   TSTOP = 1.0E-2
