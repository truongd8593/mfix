   DT_MAX = 0.1E-2
  

#
# GEOMETRY SECTION
#
  COORDINATES = 'CARTESIAN'
                                     
  XLENGTH = 50.0    IMAX = 50
  YLENGTH = 50.0    JMAX = 50
