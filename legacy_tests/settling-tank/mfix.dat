  IC_U_s(1,1)   =  0.0
  IC_V_s(1,1)   =  0.0
#
# BOUNDARY CONDITIONS SECTION
#
       !
       !            Inlet            Outlet
  BC_x_w        =    0.0             50.0
  BC_x_e        =    0.0             50.0
  BC_y_s        =   45.0             45.0
  BC_y_n        =   50.0             50.0
  BC_TYPE       = 'MASS_INFLOW' 'P_OUTFLOW'

  BC_ep_g       =    0.7

  BC_U_g        = 100.0
  BC_V_g        =   0.0

  BC_P_g        = 1.084E6    1.013E6


  BC_ROP_s(1,1) = 0.6
  BC_U_s(1,1)   =  100.0 
  BC_V_s(1,1)   =    0.0 
#
# OUTPUT CONTROL SECTION
#
   RES_DT = 0.01   
         !
         !  EP_g     P_g     U_g   U_s   ROP_s  T_g      X_g
         !           P_star  V_g   V_s          T_s      X_s       G
         !                   W_g   W_s           
   SPX_DT = 0.01     0.01    0.01  0.01  100.   100.     100.      100.  100.

